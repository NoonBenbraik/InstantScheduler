<Window x:Class="InstantScheduler.LocationSearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InstantScheduler"
        mc:Ignorable="d"
        Title="LocationSearchWindow" Height="300" Width="400"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <ProgressBar Name="progressBar" Grid.Row="0" Foreground="#DDD" Background="Transparent" BorderBrush="Transparent" IsIndeterminate="True" IsEnabled="True" Visibility="Visible"/>
        <ListBox Grid.Row="1" Name="lstLocations" MouseDoubleClick="lstLocations_MouseDoubleClick">
            
        </ListBox>
    </Grid>
</Window>
